        link(rel='stylesheet', href='/stylesheets/layoutstyle.css' type='text/css' media='all')
        link(rel='stylesheet', href='/stylesheets/ng-img-crop.css' type='text/css' media='all')
        link(rel='stylesheet', href='/angular-ui-grid/ui-grid.min.css' type='text/css' media='all')
        link(rel='stylesheet', href='/angucomplete-alt/angucomplete-alt.css')

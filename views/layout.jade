        header(data-ng-controller='LogoutController' style='z-index: 2; box-shadow:0px 0px 15px 2px rgba(0, 0, 0, 0.35); position: fixed; width: 100%; background: #cec4e1; text-align: left;  margin-top: -50px; margin-bottom: 10px; padding: 0;height:50px; color: white; font-family: sitka; font-weight: bold; ')
            a(href='/', title='Home', rel='', style='text-decoration: none; margin-left: 80px; color: #734c9e; float="left";  width="20%"; font-size: 35px;')='gluser'
